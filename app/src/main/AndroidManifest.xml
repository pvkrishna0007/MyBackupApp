<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="appsforbb.com.mybackupapp" >

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:backupAgent=".persistance.BlundellBackupAgent"
        android:theme="@style/AppTheme" >

        <meta-data android:name="com.google.android.backup.api_key" android:value="AEdPqrEAAAAIBDHoy8Nv_Tp23YumGKfUFV4k_0oHxmq5jOwXbQ" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
